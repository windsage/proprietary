
cc_library_shared {

    name: "libloc_mq_client",
    owner: "qti",
    proprietary: true,
    vendor: true,



    cflags: ["-fno-short-enums"] + GNSS_CFLAGS,

    shared_libs: [
        "libc",
        "liblog",
        "libcutils",
        "libgps.utils",
        "libloc_base_util",
