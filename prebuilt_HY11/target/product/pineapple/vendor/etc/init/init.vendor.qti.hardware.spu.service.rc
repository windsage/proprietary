on init && property:ro.boot.product.vendor.sku=pineapple
    enable vendor.qti.hardware.spu.service

on init && property:ro.boot.product.vendor.sku=niobe
    enable vendor.qti.hardware.spu.service

service vendor.qti.hardware.spu.service /vendor/bin/hw/vendor.qti.hardware.spu-service
    class hal
    user system
    group system
    disabled
