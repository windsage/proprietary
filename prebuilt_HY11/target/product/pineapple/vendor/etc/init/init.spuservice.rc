on init && property:ro.boot.product.vendor.sku=pineapple
    enable spuservice

on init && property:ro.boot.product.vendor.sku=niobe
    enable spuservice

service spuservice /vendor/bin/hw/vendor.qti.spu@2.0-service
    class hal
    user system
    group system
    disabled
