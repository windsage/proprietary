<!--Ids are used in identifying the boosts, increment ids linearly,
    do not modify Ids, or add in between-->
<!--follow the number format rules while specifying number, can be given
    in any format-->

<BoostConfigs>
    <PerfBoost>

    <!--app lauch boost-->
        <!-- CPUBOOST_MAX_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ LITTLE Core resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ PRIME Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ PRIME Core resource opcode, value-->
        <!-- POWER COLLAPSE resource opcode, value-->
        <!-- STORAGE CLK SCALING resource opcode, value-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- L3_MEMLAT_MIN_FREQ_PRIME resource opcode, value-->
        <!-- GPU_TOUCH_WAKEUP resource opcode, value-->

        <!-- Type="1", main launch boost of 2sec -->
        <Config
            Id="0x00001081" Type="1" Enable="true" Timeout="2000" Target="msmnile_au"
            Resources="0x40804000, 0xFFF, 0x40804100, 0xFFF, 0x40804200, 0xFFF,
                       0x40800000, 0xFFF, 0x40800100, 0xFFF, 0x40800200, 0xFFF,
                       0x40400000, 0x1, 0x42C10000, 0x1, 0x4303C000, 0xFFFFFF, 0x43458000, 0xFFFFFF,
                       0x42834000, 1" />

        <!-- CPUBOOST_MAX_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ LITTLE Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value-->
        <!-- POWER COLLAPSE resource opcode, value-->
        <!-- STORAGE CLK SCALING resource opcode, value-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- L3_MEMLAT_MIN_FREQ_GOLD resource opcode, value-->
        <!-- GPU_TOUCH_WAKEUP resource opcode, value-->

        <Config
            Id="0x00001081" Type="1" Enable="true" Timeout="2000" Target="poipu"
            Resources="0x40804000, 0xFFF, 0x40804100, 0xFFF,
                       0x40800000, 0xFFF, 0x40800100, 0xFFF,
                       0x40400000, 0x1, 0x42C10000, 0x1, 0x4303C000, 0xFFFFFF, 0x43480000, 0xFFFFFF,
                       0x42834000, 1" />


    <!-- config_FABoost -->
        <!-- STORAGE CLK SCALING resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ PRIME Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- L3_MEMLAT_MIN_FREQ_PRIME resource opcode, value-->

        <!-- Type="4", FA launch boost of 15sec -->
        <Config
            Id="0x00001081" Type="4" Enable="true" Timeout="15000" Target="msmnile_au"
            Resources="0x42C10000, 0x1, 0x40800200, 0x76C, 0x40800000, 0x76C, 0x40800100, 0x4B0,
                       0x4303C000, 0xFFFFFF, 0x43458000, 0xFFFFFF" />

        <!-- STORAGE CLK SCALING resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- L3_MEMLAT_MIN_FREQ_GOLD resource opcode, value-->

        <!-- Type="4", FA launch boost of 15sec -->
        <Config
            Id="0x00001081" Type="4" Enable="true" Timeout="15000" Target="poipu"
            Resources="0x42C10000, 0x1, 0x40800000, 0x76C, 0x40800100, 0x4B0,
                       0x4303C000, 0xFFFFFF, 0x43480000, 0xFFFFFF" />

    <!--Vertical Scroll boost-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ PRIME CORE resource opcode, value -->
        <!-- L3_MEMLAT_MIN_FREQ_PRIME resource opcode, value -->

        <!-- Type="1", Vertical Scroll boost -->
        <Config
            Id="0x00001080" Type="1" Enable="true" Target="msmnile_au"
            Resources="0x4303C000, 0xFFFFFF, 0x40800000, 1000, 0x40800100, 1000, 0x40800200, 1000,
                       0x43458000, 0xFFFFFF" />

        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- L3_MEMLAT_MIN_FREQ_GOLD resource opcode, value -->

        <!-- Type="1", Vertical Scroll boost -->
        <Config
            Id="0x00001080" Type="1" Enable="true" Target="poipu"
            Resources="0x4303C000, 0xFFFFFF, 0x40800000, 1000, 0x40800100, 1000,
                       0x43480000, 0xFFFFFF" />

    <!--Prefling Scroll boost-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ PRIME CORE resource opcode, value -->
        <!-- L3_MEMLAT_MIN_FREQ_PRIME resource opcode, value -->
        <!-- TIMEOUT 80ms -->
        <!-- Type="4", Prefling boost -->
        <Config
            Id="0x00001080" Type="4" Enable="true" Timeout="80" Target="msmnile_au"
            Resources="0x4303C000, 0xFFFFFF, 0x40800000, 1000, 0x40800100, 1000, 0x40800200, 1000,
                       0x43458000, 0xFFFFFF" />

        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- L3_MEMLAT_MIN_FREQ_GOLD resource opcode, value -->
        <!-- TIMEOUT 80ms -->
        <!-- Type="4", Prefling boost -->
        <Config
            Id="0x00001080" Type="4" Enable="true" Timeout="80" Target="poipu"
            Resources="0x4303C000, 0xFFFFFF, 0x40800000, 1000, 0x40800100, 1000,
                       0x43480000, 0xFFFFFF" />

    <!--Drag boost-->
        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ PRIME CORE resource opcode, value -->
        <!-- L3_MEMLAT_MIN_FREQ_PRIME resource opcode, value -->

        <!-- Type="1", Drag boost -->
        <Config
            Id="0x00001087" Type="1" Enable="true" Timeout="0" Target="msmnile_au"
            Resources="0x4303C000, 0xFFFFFF, 0x40800000, 1000, 0x40800100, 1000, 0x40800200, 1000,
                       0x43458000, 0xFFFFFF" />

        <!-- LLCC_DDR_BW_MIN_FREQ_V2 resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- L3_MEMLAT_MIN_FREQ_GOLD resource opcode, value -->

        <!-- Type="1", Drag boost -->
        <Config
            Id="0x00001087" Type="1" Enable="true" Timeout="0" Target="poipu"
            Resources="0x4303C000, 0xFFFFFF, 0x40800000, 1000, 0x40800100, 1000,
                       0x43480000, 0xFFFFFF" />

    </PerfBoost>
</BoostConfigs>
